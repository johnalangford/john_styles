</section>

<!-- COLORS -->
<section>
  <h2 id="colors">Colors</h2>
  <ul>
    <li class="color-card">
      <span class="swatch-preview red"></span>
      <ul class="swatch-details">
          <li><strong>Red</strong></li>
          <li>#ec212f</li>
          <li>$color-red</li>
      </ul>
    </li>
    <li class="color-card">
      <span class="swatch-preview error"></span>
      <ul class="swatch-details">
          <li><strong>Error Callout</strong></li>
          <li>#fd1068</li>
          <li>$color-error</li>
      </ul>
    </li>
    <li class="color-card">
      <span class="swatch-preview success"></span>
      <ul class="swatch-details">
          <li><strong>Success</strong></li>
          <li>#1ac295</li>
          <li>$color-success</li>
      </ul>
    </li>
    <li class="color-card">
      <span class="swatch-preview failure"></span>
      <ul class="swatch-details">
          <li><strong>Failure</strong></li>
          <li>#950dc0</li>
          <li>$color-failure</li>
      </ul>
    </li>
  </ul>
</section>
<section>
  <ul>
    <li class="color-card">
      <span class="swatch-preview"></span>
      <ul class="swatch-details">
          <li><strong>Dark Text</strong></li>
          <li>#cf1b41</li>
          <li>$color-red</li>
      </ul>
    </li>
    <li class="color-card">
      <span class="swatch-preview"></span>
      <ul class="swatch-details">
          <li><strong>Light Text</strong></li>
          <li>#cf1b41</li>
          <li>$color-red</li>
      </ul>
    </li>
  </ul>
</section>
<hr>

<section>
  <h2>Buttons</h2>
  <h3>Base Button</h3>
  <button>Button</button>
  <hr>
  <h3>Alt Button</h3>
  <button class="alt">Button</button>
  <hr>
  <h3>Disabled Button</h3>
  <button class="disabled">Button</button>
  <hr>
  <h3 id="txtButton">Text Button</h3>
  <a href="#">Button</a>
  <hr>
</section>

<section>
  <h2 id="inputs">Inputs</h2>
  <h3>Text Input</h3>
  <label>Default Label
    <input type="text" id="textField" placeholder="text / password / search / email">
  </label>
</section>
<section>
  <h3>Error</h3>
  <label>Default Label
    <input type="text" id="textField" placeholder="text / password / search / email">
    <div class="form-error">invalid input</div>
  </label>
</section>
<section>
  <h3>Valid</h3>
  <label>Default Label
    <input type="text" id="textField" placeholder="text / password / search / email">
    <div class="inner-icon">
      {{fa-icon "fa-check"}}
    </div>
  </label>
</section>
<section>
  <h3>Select</h3>
  <div class="select-style">
    <select>
      <option value="1">Option 1</option>
      <option value="2">Option 2</option>
      <option value="3">Option 3</option>
    </select>
  </div>
  <hr>
</section>
<section>
  <h3>Select Nested</h3>
  <div class="select-style">
    <select>
      <optgroup label="Option Group 1">
        <option value="1"><p>Option 1</p></option>
        <option value="2">Option 2</option>
        <option value="3">Option 3</option>
      </optgroup>
      <optgroup label="Option Group 2">
        <option value="1">Option 1</option>
        <option value="2">Option 2</option>
        <option value="3">Option 3</option>
      </optgroup>
    </select>
  </div>
  <hr>
</section>
<section>
  <h3 id="checkbox">Checkbox</h3>
  <label>
    <input type="checkbox" class="checkbox" name="checkboxes" value="check_1"> Checkbox 1
  </label>
  <hr>
</section>
<section>
  <h3 id="radio">Radio</h3>
  <label>
    <input type="radio" class="radio" name="radio_button" value="radio_1"> Radio 1
  </label>
  <label>
    <input type="radio" class="radio" name="radio_button" value="radio_2"> Radio 2
  </label>
  <hr>
</section>

<section>
  <h2 id="images">Images</h2>
</section>
<section>
